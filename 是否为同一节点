public static boolean IsPoint(ListNode first,ListNode head){
        ListNode forward = first;
        ListNode backward = head;
        if((first == null)||(head == null)){
            return false;
        }
        while(first != null){
            first = first.next;
        }
        while(head != null){
            head = head.next;
        }
        return first == head;
    }
