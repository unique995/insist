public static  int CircleLength (ListNode first){   
ListNode fast = first;      
ListNode slow = first;    
while((fast != null) && (fast.next != null){    
fast = fast.next.next;    
slow = slow.next;      
while (fast == slow) {      
int len = 1;       
fast = fast.next.next;      
slow = slow.next;       
while(fast != slow){       
len ++;             
fast = fast.next.next;       
slow = slow.next;       
}            
return lengthï¼›     
} 
