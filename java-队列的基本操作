public class Test {
public static void main(String[] args) {
Queue<String> queue = new LinkedList<>() ;
queue.add("A") ;
queue.add("B") ;
queue.add("C") ;
System.out.println(queue.peek()) ;
System.out.println(queue.poll()) ;
System.out.println(queue.poll()) ;
System.out.println(queue.poll()) ;
// 取完数据继续poll返回null
System.out.println(queue.poll()) ;
}
}
