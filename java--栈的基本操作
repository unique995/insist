public class Test {
public static void main(String[] args) {
Stack<String> stack = new Stack<>() ;
stack.push("A") ;
stack.push("B") ;
stack.push("C") ;
System.out.println(stack.peek()) ;
System.out.println(stack.pop()) ;
System.out.println(stack.pop()) ;
System.out.println(stack.pop()) ;
// EmptyStackException 空栈异常
System.out.println(stack.pop()) ;
}
}
